{
  "name": "fastfeet",
  "version": "1.0.0",
  "description": "GoStack Bootcamp - Challenge  Rocketseat - 10.0",
  "main": "./bin/server.ts",
  "repository": "https://github.com/KalleoPinheiro/fastfeet.git",
  "author": "Kalleo Pinheiro <kalleo.pinheiro@zup.com.br>",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage",
    "clean": "rm -rf ./yarn-lock.json ./node_modules ./dist; yarn install; yarn audit --fix",
    "dev": "nodemon ./bin/server.ts",
    "build": "yarn clean; tsc",
    "commit": "git-cz",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/bluebird": "3.5.29",
    "@types/validator": "12.0.1",
    "bcrypt": "5.0.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "global": "4.4.0",
    "helmet": "3.21.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "node-notifier": "6.0.0",
    "pg": "7.18.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.4",
    "winston": "3.2.1",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/debug": "4.1.5",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "6.1.8",
    "@types/express": "4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/multer": "^1.4.2",
    "@types/node": "13.7.1",
    "@types/node-notifier": "5.4.0",
    "@types/prettier": "1.19.0",
    "@types/sequelize": "4.28.8",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-import-helpers": "1.0.2",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "git-cz": "4.3.1",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "sequelize-cli": "5.5.1",
    "sucrase": "3.12.1",
    "ts-jest": "25.2.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.5"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "eslint . --fix",
      "prettier --write",
      "git add"
    ],
    "bin/*.{ts}": [
      "eslint . --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && npm run commit || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
